{
	"_version": "1.12.0",
	"short_name": "Überherrn",
	"name": "Überherrn App, powered by Bündnis 90/Die Grünen",
	"icons": [
		{
			"src": "img/logo-128x128.png",
			"sizes": "128x128",
			"type": "image/png"
		},
		{
			"src": "img/logo-144x144.png",
			"sizes": "144x144",
			"type": "image/png"
		},
		{
			"src": "img/logo-152x152.png",
			"sizes": "152x152",
			"type": "image/png"
		},
		{
			"src": "img/logo-192x192.png",
			"sizes": "192x192",
			"type": "image/png"
		},
		{
			"src": "img/logo-256x256.png",
			"sizes": "256x256",
			"type": "image/png"
		},
		{
			"src": "img/logo-512x512.png",
			"sizes": "512x512",
			"type": "image/png"
		}
	],
	"start_url": "/index.html",
	"scope": "/index.html",
	"background_color": "#fafafa",
	"display": "standalone",
	"sap.app": {
		"id": "gruene.ueberherrn",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"ach": "ach"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "gruene.ueberherrn.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.tnt": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "gruene.ueberherrn.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "gruene.ueberherrn.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"async": true
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": [
						"home"
					]
				},
				{
					"pattern": "StandByDuty",
					"name": "standByDuty",
					"target": [
						"standByDuty"
					]
				},
				{
					"pattern": "Numbers",
					"name": "numbers",
					"target": [
						"numbers"
					]
				},
				{
					"pattern": "Numbers/{numberType}",
					"name": "numbersDetails",
					"target": [
						"numbersDetails"
					]
				},
				{
					"pattern": "Hotels",
					"name": "hotels",
					"target": [
						"hotels"
					]
				},
				{
					"pattern": "Impressum",
					"name": "impressum",
					"target": [
						"impressum"
					]
				}
			],
			"targets": {
				"navigation": {
					"viewName": "Navigation",
					"viewId": "navigation",
					"viewLevel": 1
				},
				"home": {
					"parent": "navigation",
					"controlId": "navContainer",
					"viewName": "Home",
					"viewId": "home",
					"viewLevel": 1
				},
				"standByDuty": {
					"parent": "navigation",
					"controlId": "navContainer",
					"viewName": "StandByDuty",
					"viewId": "standByDuty",
					"viewLevel": 1
				},
				"numbers": {
					"parent": "navigation",
					"controlId": "navContainer",
					"viewName": "Numbers",
					"viewId": "numbers",
					"viewLevel": 1
				},
				"numbersDetails": {
					"parent": "navigation",
					"controlId": "navContainer",
					"viewName": "NumbersDetails",
					"viewId": "numbersDetails",
					"viewLevel": 2
				},
				"hotels": {
					"parent": "navigation",
					"controlId": "navContainer",
					"viewName": "Hotels",
					"viewId": "hotels",
					"viewLevel": 1
				},
				"impressum": {
					"parent": "navigation",
					"controlId": "navContainer",
					"viewName": "Impressum",
					"viewId": "impressum",
					"viewLevel": 1
				}
			}
		}
	}
}